{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-snappy": "^1.0",
        "bestmomo/laravel5-artisan-language": "^0.3.3",
        "davmixcool/laravel-meta-manager": "^1.2",
        "doctrine/dbal": "^3.3",
        "guzzlehttp/guzzle": "^7.4",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "josiasmontag/laravel-recaptchav3": "^1.0",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "laravelcollective/html": "^6.3",
        "league/flysystem-aws-s3-v3": "^3.0",
        "orangehill/iseed": "^3.0",
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpspreadsheet": "^1.29",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^7.0",
        "ramsey/uuid": "^4.2",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-collection-macros": "^7.10",
        "spatie/laravel-medialibrary": "^10.1",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-translatable": "6",
        "thomasjohnkane/snooze": "^2.2",
        "torann/geoip": "^3.0",
        "yajra/laravel-datatables-oracle": "^9.19"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "MaxDev\\":"MaxDev/",
            "Database\\Seeds\\": "database/seeds/"
        },
        "files":[
            "MaxDev/Helpers/Arrays.Helper.php",
            "MaxDev/Helpers/Dates.Helper.php",
            "MaxDev/Helpers/General.Helper.php",
            "MaxDev/Helpers/Images.Helper.php",
            "MaxDev/Helpers/Menu.Helpers.php",
            "MaxDev/Helpers/Query.Helper.php",
            "MaxDev/Helpers/Permission.Helper.php",
            "MaxDev/Helpers/Calculations.Helper.php",
            "MaxDev/Helpers/View.Helper.php"
        ],
        "classmap": [
            "database/seeds",
            "database/Factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "fixPdf": [
            "cp vendor/h4cc/wkhtmltopdf-amd64/bin/wkhtmltopdf-amd64 /usr/local/bin/",
            "chmod +x /usr/local/bin/wkhtmltopdf-amd64"
        ]
    }
}
